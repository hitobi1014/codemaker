<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lesson">
	
	<select id="selectLesson" parameterType="String" resultType="LessonVO">
		SELECT * FROM lesson 
			<where>
			 and les_id = #{les_id}
			</where>
	</select>
	
	<select id="selectAllLesson" resultType="LessonVO">
		SELECT * FROM lesson
	
	</select>
	
	<insert id="insertLesson" parameterType="LessonVO">
		INSERT INTO LESSON VALUES (lesson_seq.nextVal, #{les_nm}, #{les_cont}, '1', '1', '1')
	</insert>	


</mapper>