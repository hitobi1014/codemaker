<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.codemaker.teacher.resume.dao.ResumeMapper">
	<insert id="insertResume" parameterType="kr.co.codemaker.teacher.signup.vo.TeacherVO">
		INSERT INTO resume
		 VALUES (
			    'RES'||LPAD(res_SEQ.NEXTVAL,2,'0'), 'T'||LPAD(res_SEQ.NEXTVAL,4,'0'), #{resNm}, #{resTel},
			    #{resMail}, #{resProfile}, #{resMotive},
			    #{resEdu}, #{resLicense}, #{resCareer}, #{resState}
			)
	</insert>
</mapper>