<mapper namespace="kr.co.codemaker.user.board.chat.dao.ChatRoomMapper"><select id="selectAllChatRoom" resultType="kr.co.codemaker.user.board.chat.vo.ChatRoomVO">
SELECT * 
  FROM CHATROOM 
</select>
